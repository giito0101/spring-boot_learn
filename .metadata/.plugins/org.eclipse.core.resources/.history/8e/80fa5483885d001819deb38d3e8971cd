package test;

import java.util.ArrayList;
import java.util.List;

public class Test {

	public static void main(String[] args) {

		List<String> list = new ArrayList<>();
        list.add("サ");
        list.add("ム");
        list.add("ラ");
        list.add("イ");

        // forEachメソッド
        System.out.println("forEachメソッドの場合");
        list.forEach(s -> System.out.println(s));

	}

}
